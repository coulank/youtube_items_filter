{
    "name": "YouTube Items Filter",
    "author": "kouwtkz",
    "version": "1.9.5",
    "manifest_version": 3,
    "description": "This is to filter YouTube video list.",
    "icons": {
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png",
        "192": "icon192.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["script.js"],
            "run_at": "document_start"
        }
    ],
    "permissions": ["storage"],
    "options_page": "options.html"
}
