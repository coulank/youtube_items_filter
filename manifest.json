{
    "name": "YouTube Items Filter",
    "author": "綿風コウラン",
    "version": "1.4.2",
    "manifest_version": 2,
    "description": "YouTubeの動画一覧をフィルタリングすることができます",
    "content_scripts": [
        {
            "matches": [ "https://www.youtube.com/*" ],
            "js": ["script.js"],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "assets/*"
    ]
}